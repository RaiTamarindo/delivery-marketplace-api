{
    "name": "jampapp/delivery-marketplace-api",
    "type": "project",
    "description": "Delivery Marketplace API",
    "keywords": [
        "delivery",
        "marketplace",
        "api"
    ],
    "license": "UNLICENSED",
    "scripts": {
        "migrate": "php database/run_migrations.php"
    },
    "require": {
        "php": "^7.1.3",
        "illuminate/database": "^5.8",
        "slim/slim": "^3.12",
        "vlucas/phpdotenv": "^3.3",
        "middlewares/trailing-slash": "^1.1",
        "doctrine/dbal": "^2.9",
        "respect/validation": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "JampApp\\": "src/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
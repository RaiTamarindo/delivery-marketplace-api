{
    "name": "jumpapp/delivery-marketplace-api",
    "type": "project",
    "description": "Delivery Marketplace API",
    "keywords": [
        "delivery",
        "marketplace",
        "api"
    ],
    "license": "UNLICENSED",
    "scripts": {
        "migrate": "php database/run_migrations.php"
    },
    "require": {
        "php": "^7.1.3",
        "illuminate/database": "^5.8",
        "slim/slim": "^3.12",
        "oscarotero/psr7-middlewares": "^3.21",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "JumpApp\\": "src/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}